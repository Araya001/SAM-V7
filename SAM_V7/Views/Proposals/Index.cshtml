                        @if (item.Result7 == null)
                    {        <span class="glyphicon glyphicon-time" style="color:#EB984E" data-toggle="tooltip" data-original-title="Vice President for Academic Affairs for Phuket Campus"></span>}
                    else if (item.Result7.Equals("Accept"))
                    {
                            <span class="glyphicon glyphicon-ok-sign" style="color:#48C9B0" data-toggle="tooltip" data-original-title="Vice President for Academic Affairs for Phuket Campus"></span>
                        }
                        else if (item.Result7.Equals("Reject"))
                        {
                            <span class="glyphicon glyphicon-remove-sign" style="color:#C70039" data-toggle="tooltip" data-original-title="Vice President for Academic Affairs for Phuket Campus"></span>
                        }
                        else if (item.Result7.Equals("Edit"))
                        {
                            <span class="glyphicon glyphicon-info-sign" style="color:#C70039" data-toggle="tooltip" data-original-title="Vice President for Academic Affairs for Phuket Campus"></span>
                        }

                    </td>
                    <td>


                       @if (item.Result3 == null)
                       {
                        <button class="btn btn-warning" onclick="window.location.href='@Url.Action("Edit", "Proposals", new { @id = item.DocNo })'">
                            <span class="glyphicon glyphicon-pencil"></span>
                        </button>
                       }


                        <button class="btn btn-success" style="margin-left:5px" onclick="window.location.href='@Url.Action("Details", "Proposals", new { id = item.DocNo})'">
                            <span class="glyphicon glyphicon-list-alt"></span>
                        </button>

                        @if (item.Result3 == null)
                        {



                            <button class="btn btn-danger" style="margin-left:5px" onclick="window.location.href='@Url.Action("Delete", "Proposals", new { @id = item.DocNo, @did=item.Files.First(f => f.FileType == SAM_V7.Models.FileType.Avatar).FileId})'">
                                <span class="glyphicon glyphicon-trash"></span>
                            </button>
                        }


                        <button class="btn btn-primary" style="margin-left:5px"  onclick="location.href='@Url.Action("Remark", "Proposals", new { id = item.DocNo })'">
                            <span class="glyphicon glyphicon-exclamation-sign"></span>
                        </button>


                    </td>
                </tr>
            }

        </table>
    
}
